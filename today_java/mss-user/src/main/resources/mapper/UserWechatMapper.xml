<?xml version="1.0" encoding="UTF-8"?>
        <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.microservice.skeleton.user.mapper.UserWechatMapper">

    <select id="selectByOpenid" resultType="com.microservice.skeleton.user.domain.entity.User.UserWechat">
        SELECT * FROM sys_user_wechat WHERE openid = #{openid} LIMIT 1
    </select>

    <select id="selectByUserId" resultType="com.microservice.skeleton.user.domain.entity.User.UserWechat">
        SELECT * FROM sys_user_wechat WHERE user_id = #{userId} LIMIT 1
    </select>

</mapper>
